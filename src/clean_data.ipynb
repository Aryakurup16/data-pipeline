    "import pandas as pd\n",
    "\n",
    "def clean_data():\n",
    "    df = pd.read_csv(\"sales_data_200.csv\")\n",
    "    df = df.dropna()\n",
    "    df['total_amount'] = df['quantity'] * df['price']\n",
    "    df.to_csv(\"clean_sales_data.csv\", index=False)\n",
    "    print(\"Data cleaned successfully!\")\n",
    "\n",
    "clean_data()\n"
   ]
